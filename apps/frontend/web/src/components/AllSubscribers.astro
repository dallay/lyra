---
import { Breadcrumb } from '@lyra/ui';
import CrudSubscriberDataTable from './CrudSubscriberDataTable.vue';
import { type BreadcrumbItem } from '@lyra/vm-core';

const breadcrumbItems: BreadcrumbItem[] = [
 {
  label: 'Home',
  href: '/app',
 } as BreadcrumbItem,
 {
  label: 'Audience',
  href: '/app/audience',
  icon: 'ph:users-duotone',
 } as BreadcrumbItem,
 {
  label: 'Subscribers',
  href: '/app/audience/subscribers',
  icon: 'ph:users-three-duotone',
 } as BreadcrumbItem,
];
---

<CrudSubscriberDataTable client:load>
	<Breadcrumb items={breadcrumbItems} slot="header" />
</CrudSubscriberDataTable>
