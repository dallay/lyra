{
  "name": "@lyra/ui-vue",
  "description": "Lyra vue ui components",
  "version": "0.0.1",
  "private": true,
  "author": "Yuniel Acosta <yunielacosta738@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/ui-vue.umd.js",
  "module": "./dist/ui-vue.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-vue.mjs",
      "require": "./dist/ui-vue.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./*": "./dist/*"
  },
  "keywords": [
    "vue",
    "common ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yacosta738/lyra.git",
    "directory": "apps/frontend/packages/ui-vue",
    "branch": "main"
  },
  "bugs": {
    "url": "https://github.com/yacosta738/lyra/issues"
  },
  "homepage": "lyra-nwhm.onrender.com",
  "scripts": {
    "build:types": "vue-tsc --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "build:vite": "vite build",
    "dev": "echo \"DEV: ðŸŸ¢ Starting with dev mode\" && npm-run-all build:vite dev:ui",
    "dev:ui": "vite",
    "build": "npm-run-all --parallel build:vite build:types",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.x"
  },
  "dependencies": {
    "@lyra/vm-core": "workspace:*",
    "@vueuse/core": "^10.8.0",
    "classnames": "^2.5.1",
    "vue": "^3.4.19",
    "vue-router": "4"
  },
  "devDependencies": {
    "@lyra/css-config": "workspace:*",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.17",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.3.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "jsdom": "^24.0.0",
    "sass": "^1.71.1",
    "tsc-alias": "1.8.7",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "pnpm@8.14.1"
}
