{
  "extends": "tsconfig/astro.json",
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "esnext",
    "module": "esnext",
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "/Users/acosta/Dev/lyra/apps/frontend/web",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "~/*": [
        "src/*"
      ],
      "atoms:*": [
        "components/atoms/*.astro"
      ],
      "molecules:*": [
        "components/molecules/*.astro"
      ],
      "organisms:*": [
        "components/organisms/*.astro"
      ],
      "layouts:*": [
        "layouts/*.astro"
      ]
    },
    "jsx": "preserve"
  }
}