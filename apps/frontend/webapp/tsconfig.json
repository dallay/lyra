{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["src/*"],
      "atoms:*": ["components/atoms/*.astro"],
      "molecules:*": ["components/molecules/*.astro"],
      "organisms:*": ["components/organisms/*.astro"],
      "layouts:*": ["layouts/*.astro"]
    }
  }
}
